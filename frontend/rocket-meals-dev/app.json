{
  "expo": {
    "name": "studi-futter",
    "slug": "rocket-meals-studi-futter",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "notification": {
        "icon": "./assets/images/notification-icon.png"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/rocket-meals"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "461671f9-774f-4bc4-80a8-5601313539b0"
      }
    },
    "owner": "baumgartner-software"
  }
}
