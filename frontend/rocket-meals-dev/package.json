{
  "name": "rocket-meals-dev",
  "homepage": "/rocket-meals",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:offline": "expo start --offline",
    "production": "expo start --no-dev --minify",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "deploy:gh:pages": "npm run export:web && gh-pages -t -d dist",
    "deploy:local": "npm run export:web:dev && mkdir -p ./serveDist/rocket-meals && cp -r ./dist/* ./serveDist/rocket-meals/ && npx serve serveDist",
    "export:web": "expo export --platform web",
    "export:web:dev": "expo export --platform web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "jest": {
    "preset": "jest-expo"
  },
