{
  "name": "rocket-meals-dev",
  "homepage": "/rocket-meals",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:offline": "expo start --offline",
    "start:production": "expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:offline": "expo start --web --offline",
    "deploy:gh:pages": "npm run export:web && gh-pages -t -d dist",
    "deploy:local": "npm run export:web:dev && mkdir -p ./serveDist/rocket-meals && cp -r ./dist/* ./serveDist/rocket-meals/ && npx serve serveDist",
    "export:web": "expo export --platform web",
    "export:web:dev": "expo export --platform web",
    "test": "jest --watchAll",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "postinstall": "patch-package && npm run licenseCrawler",
    "licenseCrawler": "npx npm-license-crawler -onlyDirectDependencies -json licenses.json"
  },
  "jest": {
    "preset": "jest-expo"
  },
